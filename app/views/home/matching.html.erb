<div class="d-flex flex-column align-items-center">
  <div>
   내가 좋아요 누른 유저 : 
      <% @liked_users.each do |liked_user|%>
    <div class="user-box d-flex justify-content-around align-items-center">
        <%= image_tag liked_user[0].image.thumb , style:"width:180px;height:180px;"%>
        <div class="d-flex flex-column justify-content-around align-self-">
            <div>
            <%= liked_user[0].dog_name%>
            <%= liked_user[0].dog_kind%>
            <%=  liked_user[0].is_male_dog? ? "수컷" : "암컷"%>
            </div>
            <div>
            <%= @recent_messages[liked_user[0].id].content%>
            </div>
        </div>
     
        <%= link_to '좋아요 취소!', like_delete_path(current_user.id, liked_user[0].id), method: :delete%>
      <% if liked_user[1]%>
        <%= link_to '매칭됨!!!!', post_new_match_path(liked_user[1], current_user.id, liked_user[0].id)%>
      <% end%>
    </div>
    <br>
      <% end%>
  </div>
</div>


